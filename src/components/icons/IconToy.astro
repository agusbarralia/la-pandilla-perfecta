---
interface Props {
  class?: string;
}

const { class: className = "w-12 h-12" } = Astro.props;
---

<svg
  class={className}
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width="2"
  stroke-linecap="round"
  stroke-linejoin="round"
>
  <circle cx="12" cy="12" r="8" />
  <path d="M8 12h8" />
  <path d="M12 8v8" />
  <circle cx="12" cy="12" r="3" fill="currentColor" />
</svg>
